{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext"],
    "types": ["@cloudflare/workers-types"],
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "path": ["./path.js"]
    }
  },
  "include": ["src/**/*", "bindings.d.ts"],
  "references": [
    {
      "path": "."
    },
    {
      "path": "ascella-web"
    },
    {
      "path": "common"
    }
  ]
}
