{
  "name": "ascella",
  "version": "1.0.0",
  "description": "Example project using Miniflare, TypeScript, esbuild and Jest",
  "type": "module",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "node build.js",
    "dev": "miniflare --live-reload --debug --modules dist/index.mjs",
    "dev:remote": "wrangler dev",
    "types:check": "tsc && tsc -p test/tsconfig.json",
    "deploy": "wrangler publish",
    "test": "npm run build && node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "workspaces": [
    ".",
    "ascella-embeds/",
    "ascella-web/",
    "common/"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^3.16.0",
    "@miniflare/d1": "^2.9.0",
    "@miniflare/shared": "^2.9.0",
    "@types/mime-types": "^2.1.1",
    "common": "workspace:./common",
    "d1-orm": "^0.2.1",
    "discord-interactions": "^3.2.0",
    "discordeno": "^16.0.1",
    "esbuild": "^0.15.9",
    "hono": "^2.2.1",
    "mime-types": "^2.1.35",
    "miniflare": "^2.9.0",
    "prettier": "^2.7.1",
    "turbo": "^1.5.2",
    "typescript": "^4.8.3",
    "ulid-workers": "^2.0.0",
    "wrangler": "^2.1.6"
  }
}
